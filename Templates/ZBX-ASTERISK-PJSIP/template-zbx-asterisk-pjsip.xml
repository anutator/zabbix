<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
    <version>4.4</version>
    <date>2020-02-04T08:31:45Z</date>
    <groups>
        <group>
            <name>Templates</name>
        </group>
    </groups>
    <templates>
        <template>
            <template>Template App Asterisk PJSIP</template>
            <name>Template App Asterisk PJSIP</name>
            <groups>
                <group>
                    <name>Templates</name>
                </group>
            </groups>
            <applications>
                <application>
                    <name>Asterisk</name>
                </application>
            </applications>
            <items>
                <item>
                    <name>Число активных вызовов</name>
                    <key>asterisk[calls.active]</key>
                    <delay>30s</delay>
                    <applications>
                        <application>
                            <name>Asterisk</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>Число входящих вызовов</name>
                    <key>asterisk[calls.incoming]</key>
                    <delay>30</delay>
                    <applications>
                        <application>
                            <name>Asterisk</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>Число исходящих вызовов</name>
                    <key>asterisk[calls.outgoing]</key>
                    <delay>30</delay>
                    <applications>
                        <application>
                            <name>Asterisk</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>Число обрабатываемых вызовов на Asterisk</name>
                    <key>asterisk[calls.processed]</key>
                    <delay>5m</delay>
                    <history>30d</history>
                    <applications>
                        <application>
                            <name>Asterisk</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>Число активных каналов</name>
                    <key>asterisk[channels.active]</key>
                    <delay>30s</delay>
                    <applications>
                        <application>
                            <name>Asterisk</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>Общее число SIP пиров на Asterisk</name>
                    <key>asterisk[sip.peers]</key>
                    <delay>30</delay>
                    <history>7d</history>
                    <applications>
                        <application>
                            <name>Asterisk</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>Проверить статус транка</name>
                    <key>asterisk[sip.trunk.down]</key>
                    <delay>30s</delay>
                    <applications>
                        <application>
                            <name>Asterisk</name>
                        </application>
                    </applications>
                    <valuemap>
                        <name>Service state</name>
                    </valuemap>
                </item>
                <item>
                    <name>Последняя перезагрузка (reload) конфигурационных файлов Asterisk</name>
                    <key>asterisk[status.reload]</key>
                    <delay>1h</delay>
                    <history>30d</history>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <applications>
                        <application>
                            <name>Asterisk</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>Время работы Asterisk с момента последней перезагрузки</name>
                    <key>asterisk[status.uptime]</key>
                    <history>30d</history>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <applications>
                        <application>
                            <name>Asterisk</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>Версия Asterisk</name>
                    <key>asterisk[status.version]</key>
                    <delay>1d</delay>
                    <history>30d</history>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <applications>
                        <application>
                            <name>Asterisk</name>
                        </application>
                    </applications>
                </item>
                <item>
                    <name>Статус порта UDP номер $1</name>
                    <key>net.udp.listen[5060]</key>
                    <delay>60</delay>
                    <history>7d</history>
                    <applications>
                        <application>
                            <name>Asterisk</name>
                        </application>
                    </applications>
                    <valuemap>
                        <name>Service state</name>
                    </valuemap>
                    <triggers>
                        <trigger>
                            <expression>{last(0)}=0</expression>
                            <name>SIP порт не слушает на {HOST.NAME}</name>
                            <priority>DISASTER</priority>
                            <dependencies>
                                <dependency>
                                    <name>Процесс asterisk не запущен на {HOST.NAME}</name>
                                    <expression>{Template App Asterisk PJSIP:proc.num[asterisk].last(0)}=0</expression>
                                </dependency>
                            </dependencies>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <name>Число $1 процессов</name>
                    <key>proc.num[asterisk]</key>
                    <history>7d</history>
                    <applications>
                        <application>
                            <name>Asterisk</name>
                        </application>
                    </applications>
                    <triggers>
                        <trigger>
                            <expression>{last(0)}=0</expression>
                            <name>Процесс asterisk не запущен на {HOST.NAME}</name>
                            <priority>DISASTER</priority>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <name>Контрольная сумма файла $1</name>
                    <key>vfs.file.cksum[/etc/asterisk/asterisk.conf]</key>
                    <delay>15m</delay>
                    <history>7d</history>
                    <applications>
                        <application>
                            <name>Asterisk</name>
                        </application>
                    </applications>
                    <triggers>
                        <trigger>
                            <expression>{diff(0)}&gt;0</expression>
                            <name>/etc/asterisk/asterisk.conf изменен на {HOST.NAME}</name>
                            <priority>INFO</priority>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <name>Контрольная сумма плана нумерации $1</name>
                    <key>vfs.file.cksum[/etc/asterisk/extensions.conf]</key>
                    <delay>15m</delay>
                    <history>7d</history>
                    <applications>
                        <application>
                            <name>Asterisk</name>
                        </application>
                    </applications>
                    <triggers>
                        <trigger>
                            <expression>{diff(0)}&gt;0</expression>
                            <name>/etc/asterisk/extensions.conf изменен на {HOST.NAME}</name>
                            <status>DISABLED</status>
                            <priority>INFO</priority>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <name>Контрольная сумма основных настроек SIP $1</name>
                    <key>vfs.file.cksum[/etc/asterisk/pjsip.conf]</key>
                    <delay>15m</delay>
                    <history>7d</history>
                    <applications>
                        <application>
                            <name>Asterisk</name>
                        </application>
                    </applications>
                    <triggers>
                        <trigger>
                            <expression>{diff(0)}&gt;0</expression>
                            <name>/etc/asterisk/pjsip.conf изменен на {HOST.NAME}</name>
                            <status>DISABLED</status>
                            <priority>INFO</priority>
                        </trigger>
                    </triggers>
                </item>
                <item>
                    <name>Контрольная сумма шаблонов телефонов и транков $1</name>
                    <key>vfs.file.cksum[/etc/asterisk/pjsip_wizard.conf]</key>
                    <delay>15m</delay>
                    <history>7d</history>
                    <applications>
                        <application>
                            <name>Asterisk</name>
                        </application>
                    </applications>
                    <triggers>
                        <trigger>
                            <expression>{diff(0)}&gt;0</expression>
                            <name>/etc/asterisk/pjsip_wizard.conf изменен на {HOST.NAME}</name>
                            <status>DISABLED</status>
                            <priority>INFO</priority>
                        </trigger>
                    </triggers>
                </item>   
            </items>
            <discovery_rules>
                <discovery_rule>
                    <name>SIP транки</name>
                    <key>ast.pjsip.discover</key>
                    <delay>1h</delay>
                    <item_prototypes>
                        <item_prototype>
                            <name>Латентность транка {#TRUNKNAME}</name>
                            <key>ast.pjsip.latency[{#TRUNKNAME}]</key>
                            <value_type>FLOAT</value_type>
                            <units>ms</units>
                            <applications>
                                <application>
                                    <name>Asterisk</name>
                                </application>
                            </applications>
                        </item_prototype>
                    </item_prototypes>
                </discovery_rule>
            </discovery_rules>
        </template>
    </templates>
    <graphs>
        <graph>
            <name>Активные вызовы</name>
            <graph_items>
                <graph_item>
                    <color>1A7C11</color>
                    <item>
                        <host>Template App Asterisk PJSIP</host>
                        <key>asterisk[calls.active]</key>
                    </item>
                </graph_item>
            </graph_items>
        </graph>
        <graph>
            <name>Активные каналы</name>
            <graph_items>
                <graph_item>
                    <color>0080FF</color>
                    <item>
                        <host>Template App Asterisk PJSIP</host>
                        <key>asterisk[channels.active]</key>
                    </item>
                </graph_item>
            </graph_items>
        </graph>
        <graph>
            <name>Входящие/Исходящие вызовы</name>
            <graph_items>
                <graph_item>
                    <color>BF00FF</color>
                    <item>
                        <host>Template App Asterisk PJSIP</host>
                        <key>asterisk[calls.incoming]</key>
                    </item>
                </graph_item>
                <graph_item>
                    <sortorder>1</sortorder>
                    <color>FF8000</color>
                    <item>
                        <host>Template App Asterisk PJSIP</host>
                        <key>asterisk[calls.outgoing]</key>
                    </item>
                </graph_item>
            </graph_items>
        </graph>
        <graph>
            <name>Число обрабатываемых вызовов</name>
            <graph_items>
                <graph_item>
                    <color>26A69A</color>
                    <item>
                        <host>Template App Asterisk PJSIP</host>
                        <key>asterisk[calls.processed]</key>
                    </item>
                </graph_item>
            </graph_items>
        </graph>
    </graphs>
    <value_maps>
        <value_map>
            <name>Service state</name>
            <mappings>
                <mapping>
                    <value>0</value>
                    <newvalue>Down</newvalue>
                </mapping>
                <mapping>
                    <value>1</value>
                    <newvalue>Up</newvalue>
                </mapping>
            </mappings>
        </value_map>
    </value_maps>
</zabbix_export>
